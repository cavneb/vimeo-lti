{{#if isLoaded}}
<div id="video">
  <div class="above_link clearfix">
    <a href="javascript:history.back();" class="back_button">&laquo; search results</a>
    <a {{bindAttr href="chooseVideoLink"}} class="btn btn-mini btn-primary pull-right">Embed Video</a>
  </div>

  <div class="flex-video widescreen vimeo">
    {{{oembed.html}}}
  </div>

  {{partial 'data'}}
</div>

{{else}}
{{partial 'loader'}}
{{/if}}
